#include<stdio.h>
int main()
{
  int n;
  int a;
  int i;
  scanf("%d %d",&n,&a);
  int b[n];
  for(i=0;i<n;i++)
    {
    scanf("%d",&b[i]);
    }
  int c=0;
  for(i=0;i<n;i++)
    {
    if(b[i]==a)
      {
      c++;
      }
    }
  if(c<0)
    {
    printf("-1");
    }
  else
    {
    printf(c+1);
    }
return 0;
  }
